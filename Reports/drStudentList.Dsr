Private Sub DataReport_Initialize()
    Me.Sections("Section4").Controls("lblDate").Caption = MonthName(Month(Now)) & " " & Day(Now) & ", " & Year(Now)
    Me.Sections("Section4").Controls("lblSchoolName").Caption = CurrentSchool.SchoolName
